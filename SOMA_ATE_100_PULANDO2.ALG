algoritmo "semnome"
var
   c, soma: inteiro
inicio
c <- 0
soma <- 0
       enquanto (c <= 100) faca
         escreva (c," + ")
         c <- c + 2
         soma <- soma + c
       fimenquanto
       escreval (" ")
       escreval ("--------------------------")
       escreval ("RESULTADO DA SOMA: ", soma)
fimalgoritmo