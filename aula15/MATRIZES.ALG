algoritmo "semnome"
var
   mat: vetor[1..3,1..2] de inteiro
   l,c :inteiro
inicio
   para l <- 1 ate 3 passo 1 faca
              para c <- 1 ate 2 passo 1 faca
                 escreva  ("DIGITE O VALOR DA POS. [",l,",",c,"] 3:":2)
                  leia(mat[l,c])
              fimpara
   fimpara
   
   para l <- 1 ate 3 passo 1 faca
              para c <- 1 ate 2 passo 1 faca
                  escreva(mat[l,c]:2)
              fimpara
              escreval ()
   fimpara
fimalgoritmo