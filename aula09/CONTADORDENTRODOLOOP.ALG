algoritmo "semnome"
var
   c,n,soma: inteiro
inicio
      c <- 1
      soma <- 0
      enquanto (c <= 5) faca
      escreva ("escolha o",c,"º numero:")
      leia (n)
      soma <- soma + n
      c <- c + 1
      fimenquanto
      escreval ("O VALOR DA SOMA DOS NUMEROS DIGITADOS É: ", soma)
fimalgoritmo