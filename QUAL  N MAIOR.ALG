algoritmo "semnome"
var
   n1, n2: inteiro
inicio
   escreva ("PRIMEIRO NUM.: ")
   leia (n1)
   escreva ("SEGUNDO NUM.: ")
   leia (n2)

   se (n1 > n2) entao
      escreva ("O PRIMEIRO NUMERO É MAIOR")
   senao
    se (n1 < n2) entao
      escreva ("O SEGUNDO NUMERO É MAIOR")
      senao
      escreva ("NENHUM NUMERO É MAIOR")
   fimse
fimse
fimalgoritmo